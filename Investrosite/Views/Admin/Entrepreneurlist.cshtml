@model List<Investrosite.Models.Database.Entrepreneur>
@{
    ViewBag.Title = "Entrepreneur List";
}

<center><h2>Entrepreneur List</h2></center>
<div class="container mt-5">
    <div class="d-flex justify-content-center row">
        <div class="col-md-8">
            <table class="table table-bordered">
                <tr>
                    <th> ID</th>
                    <th> Name</th>
                    <th> Email</th>
                    <th> Phone</th>
                    <th> Role</th>
                    <th></th>
                    <th></th>
                </tr>
                @foreach (var p in Model)
                {
                    <tr>
                        <td> @p.Id</td>
                        <td> @p.Name</td>
                        <td> @p.Email</td>
                        <td> @p.Phone</td>
                        <td> @p.Role</td>
                        <td><a href="/Admin/Edit/@p.Id">Edit</a></td>
                        <td><a href="/Admin/DeleteEntrepreneur/@p.Id">Delete</a></td>
                    </tr>
                 }
                </table>

</div>
    </div>
    </div>